  "devDependencies": {
    "@types/atob": "^2.1.2",
    "@types/btoa": "^1.2.3",
    "@types/qrcode": "^1.5.0",
    "@types/qrcode-terminal": "^0.12.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.9.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
